{
    "name": "williarin/wordpress-anonymizer",
    "description": "Anonymize a WordPress database without a WordPress installation",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "William Arin",
            "email": "williamarin.dev@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "doctrine/dbal": "^3.3",
        "fakerphp/faker": "^1.18",
        "symfony/config": "^6.0",
        "symfony/console": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/dotenv": "^6.0",
        "symfony/string": "^6.0",
        "symfony/yaml": "^6.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.23",
        "kubawerlos/php-cs-fixer-custom-fixers": "^3.7",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "symfony/var-dumper": "^6.0",
        "symplify/coding-standard": "^10.0",
        "symplify/easy-coding-standard": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Williarin\\WordpressAnonymizer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Williarin\\WordpressAnonymizer\\Test\\": "test/"
        }
    }
}
